{
  "manifest_version": 2,
  "name": "VK Remove Stories",
  "version": "0.0.0.3",
  "description": "VK Remove Stories",

  "icons": { "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png" },

  "content_scripts": [
    {
      "matches": [
        "https://vk.com/*"
      ],
      "css": [
        "style.css"
      ],
      "run_at": "document_idle"
    }
  ]
}